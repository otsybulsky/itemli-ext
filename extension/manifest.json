{
  "applications": {
    "gecko": {
      "id": "ext@itemli.com"
    }
  },
  "manifest_version": 2,
  "name": "itemli-ext",
  "version": "0.1",

  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "images/icons8-add-link-48.png",
      "96": "images/icons8-add-link-96.png"
    },
    "default_title": "Itemli Ext",
    "default_popup": "popup.html"
  },

  "permissions": ["tabs"]
}
